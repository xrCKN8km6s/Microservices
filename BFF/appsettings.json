{
  "Serilog": {
    "Using": "Serilog.Sinks.Console",
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "System": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": { "Application": "BFF" }
  },
  "AllowedHosts": "*",
  "WebUrl": "http://localhost:4200",
  "identityUrl": "http://localhost:3000",
  "identityUrlInternal": "http://localhost:3000",
  "redisConfig": "localhost",
  "usersUrl": "http://localhost:5100/",
  "ordersUrl": "http://localhost:5200/",
  "validIssuers": [
    "http://localhost:3000"
  ]
}