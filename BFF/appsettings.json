{
  "Serilog": {
    "Using": "Serilog.Sinks.Console",
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "System": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": { "Application": "BFF" }
  },
  "AllowedHosts": "*",
  "WebUrl": "http://localhost:4200"
}