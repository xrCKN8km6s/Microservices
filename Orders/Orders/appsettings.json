{
  "Serilog": {
    "Using": "Serilog.Sinks.Console",
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "System": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": ["FromLogContext"],
    "Properties": { "Application": "Orders" }
  },
  "AllowedHosts": "*",
  "WebUrl": "http://localhost:4200",
  "ConnectionString": "Host=localhost;Database=MicroserviceDb;Username=db_user;Password=db_pass"
}