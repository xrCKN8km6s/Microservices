{
  "Serilog": {
    "Using": "Serilog.Sinks.Console",
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Debug"
      }
    },
    "WriteTo": [{ "Name": "Console" }],
    "Enrich": ["FromLogContext"],
    "Properties": { "Application": "EntryPoint" }
  },
  "AllowedHosts": "*"
}